{
  "name": "Chrome Photo Checker",
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enable image check"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab"
  ]
}
